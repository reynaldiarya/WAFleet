{
  "name": "wafleet",
  "version": "0.0.1",
  "description": "**WAFleet** is an unofficial WhatsApp API built on top of the reverse-engineered **WhatsApp Web** protocol via **[Baileys](https://github.com/WhiskeySockets/Baileys)**. It lets you automate messaging, manage sessions, and integrate WhatsApp into your systems through a lightweight REST API.",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && shx cp .env.example dist/ && shx cp package.json dist/",
    "start": "node dist/server.js",
    "prettier-format": "prettier --config .prettierrc --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@types/qrcode": "^1.5.5",
    "baileys": "^6.7.18",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "multer": "^2.0.2",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "prettier": "^3.6.2",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.17.2",
    "shx": "^0.4.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
